
@{
    ViewBag.Title = "Integration Result";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var data = (MNproject.Models.Classes.Data)Session["data"];
    var result = (double)Session["resultSimpson"];
    var addFxi = 0.0;
    var addSimpsonXi = 0.0;
}

<h3>Result for:  fx=  @data.fx ; a = @data.a ; b = @data.b ; n = @data.n  | <span style="color:orangered">Ans: @result</span></h3>
<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th>n</th>
                <th>Xi</th>
                <th>Fxi</th>
                <th>Simpson 1/3</th>
            </tr>
        </thead>
        <tbody>
            @foreach (MNproject.Models.Classes.Simpson s in (List<MNproject.Models.Classes.Simpson>)Session["tableSimpson"])
            {
                <tr>
                    <td>@s.n</td>
                    <td>@s.x</td>
                    <td>@s.fx</td>
                    <td>@s.simpsonXi</td>
                </tr>
                addSimpsonXi += s.simpsonXi;
                addFxi += s.fx;
            }            
        </tbody>
    </table>
    
</div>